{
    "name": "ui",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "typecheck": "yarn tsc",
        "build-electron": "python ./scripts/build.py ./scripts/electron.build",
        "build-web": "python ./scripts/build.py ./scripts/web.build",
        "start-electron": "yarn build-web && yarn build-electron && yarn electron --trace-warnings build/electron.js",
        "start": "yarn build-web && python3 -m http.server --directory build 8080",
        "format": "yarn prettier --write *.ts *.tsx"
    },
    "dependencies": {
        "chart.js": "^3.3.2",
        "electron": "^13.0.1",
        "papaparse": "^5.3.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@types/electron": "^1.6.10",
        "@types/papaparse": "^5.2.5",
        "@types/react": "^17.0.9",
        "@types/react-dom": "^17.0.6",
        "esbuild": "^0.12.7",
        "prettier": "^2.3.0",
        "typescript": "^4.3.2"
    }
}
