setenv UI_CONFIG_OVERRIDES "window.DS_CONFIG_MODE = 'server';"

yarn build-ui

cd server

yarn esbuild exporter.ts --sourcemap --platform=node --bundle --target=node10.4 --external:pg-native --external:canvas --external:jsdom --outfile=../build/exporter.js
yarn esbuild index.ts --sourcemap --platform=node --bundle --target=node10.4 --external:pg-native --outfile=../build/server.js

setenv_default SERVER_CONFIG_OVERRIDES ""
prepend {SERVER_CONFIG_OVERRIDES} ../build/server.js