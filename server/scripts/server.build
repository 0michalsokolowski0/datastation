setenv UI_CONFIG_OVERRIDES "window.DS_CONFIG_MODE = 'server';"

yarn build-ui

yarn esbuild server/exporter.ts --sourcemap --platform=node --bundle --target=node10.4 --external:pg-native --external:canvas --external:jsdom --external:react-native-fs --external:react-native-fetch-blob --outfile=./build/exporter.js
yarn esbuild server/index.ts --sourcemap --platform=node --bundle --target=node10.4 --external:pg-native --external:react-native-fs --external:react-native-fetch-blob --outfile=./build/server.js

setenv_default SERVER_CONFIG_OVERRIDES ""
prepend {SERVER_CONFIG_OVERRIDES} ./build/server.js